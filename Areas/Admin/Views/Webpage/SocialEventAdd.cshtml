@model MrCMS.Web.Apps.SocialEvents.Pages.SocialEvent

<div class="form-group">
    @Html.LabelFor(model => model.FeatureImage, "Social Event Image") <br />
    @Html.TextBoxFor(model => model.FeatureImage, new { data_type = "media-selector" })
</div>
<div class="form-group">
    @Html.LabelFor(model => model.Location, "Location") <br />
    @Html.TextBoxFor(model => model.Location)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.BeginsAt, "Begins At") <br />
    @Html.EditorFor(model => model.BeginsAt)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.EndsAt, "Ends At") <br />
    @Html.EditorFor(model => model.EndsAt)
</div>

<div class="form-group">
    @Html.LabelFor(model => model.Website, "Website") <br />
    @Html.TextBoxFor(model => model.Website)
</div>